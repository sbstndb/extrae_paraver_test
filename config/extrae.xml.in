<?xml version="1.0" encoding="UTF-8"?>

<trace enabled="yes"
       type="paraver"
       home="@EXTRAE_HOME@"
       initial-mode="detail">

  <mpi enabled="yes">
    <comm-calls enabled="yes" />
    <counters enabled="no" />
  </mpi>

  <openmp enabled="no" ompt="no" />
  <pthread enabled="no" />

  <user-functions enabled="yes" exclude-automatic-functions="yes">
    <counters enabled="no" />
  </user-functions>

  <callers enabled="no" />
  <counters enabled="no" />
  <dynamic-memory enabled="no" />
  <input-output enabled="no" />
  <syscall enabled="no" />
  <sampling enabled="no" />
  <buffer enabled="yes">
    <size enabled="yes">4000000</size>
    <circular enabled="no" />
  </buffer>

  <storage enabled="yes">
    <trace-prefix enabled="yes">trace</trace-prefix>
  </storage>

  <merge enabled="no"
         synchronization="default"
         tree-fan-out="8"
         max-memory="256"
         joint-states="yes"
         keep-mpits="yes"
         translate-addresses="yes"
         sort-addresses="yes"
         overwrite="yes" />

</trace>
